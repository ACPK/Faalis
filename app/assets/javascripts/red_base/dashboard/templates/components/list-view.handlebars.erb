  <ul>
    {{#unless model}}
    <li><%= _("There is not record") %></li>
    {{/unless}}
    {{#each record in model}}

    <li {{bind-attr class="record.is_selected:selected :pointer"}}>
      <i {{action "toggle_details" record}} {{bind-attr class="record.view_details:fa-rotate-90 :handle :fa :fa-angle-right"}}></i>
      <div class="title" {{ action "select_item" record}}> {{ __title__  record title-field}}</div>
      <div class="buttons text-right"><span><i class="fa fa-edit"></i></span></div>
      <div {{bind-attr class="record.view_details:show_details:hide_details :details"}}>
        asdfsdf
        sdfsdf
        sdfsdfs
      </div>
    </li>
    {{/each}}
    <li class="table-summery">
      <div class="small-6 columns">
        <span>There is {{ asd }}</span>
      </div>
      <div class="small-6 columns text-right">
        <span class="btn yellow tiny"><%= _("Toggle Select") %></span>
        <span class="btn blue tiny"><%= _("Select All") %></span>
        <span class="btn red tiny" {{ action 'delete'}}> <%= _("Delete") %></span>
      </div>
    </li>
  </ul>
